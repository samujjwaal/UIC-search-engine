<script type="text/javascript">
// DOM ready
jQuery(document).ready(function(){

// Create the dropdown base
jQuery("<select />").appendTo("#topmenu");
jQuery("<select />").appendTo("#globalmenu");

// Create default option "Go to..."
jQuery("<option />", {
 "selected": "selected",
 "value"   : "",
 //"text"    : "Menu Selection..."
 "text"    : "Campuses and Colleges..."
}).appendTo("#topmenu select");
jQuery("<option />", {
 "selected": "selected",
 "value"   : "",
 "text"    : "Menu Selection..."
}).appendTo("#globalmenu select");

// Populate dropdown with menu items
/*jQuery("#topmenu a").each(function() {
var el = jQuery(this);
jQuery("<option />", {
   "value"   : el.attr("href"),
   "text"    : el.text()
}).appendTo("#topmenu select");
});*/

jQuery("#globalmenu a").each(function() {
var el = jQuery(this);
if ( el.parent().parent().attr("class") != "sub-menu" ){
jQuery("<option />", {
   "value"   : el.attr("href"),
   "text"    : el.text()
}).appendTo("#globalmenu select");
}
});

jQuery("#topmenu a").each(function() {
var el = jQuery(this);
if ( el.parent().parent().attr("id") == "menu-eyebrow" ){
jQuery("<option />", {
  "value"   : el.attr("href"),
  "text"    : el.text()
}).appendTo("#globalmenu select");
}
});

// To make dropdown actually work
// To make more unobtrusive: http://css-tricks.com/4064-unobtrusive-page-changer/
jQuery("#topmenu select").change(function() {
window.location = jQuery(this).find("option:selected").val();
});
jQuery("#globalmenu select").change(function() {
window.location = jQuery(this).find("option:selected").val();
});
jQuery(".tweet").tweet({
            username: "",
            join_text: "auto",
            avatar_size: 0,
            count: 3,
            auto_join_text_default: "we said,", 
            auto_join_text_ed: "we",
            auto_join_text_ing: "we were",
            auto_join_text_reply: "we replied to",
            auto_join_text_url: "we were checking out",
            loading_text: "loading tweets..."
        });
		
jQuery('.slider').flexslider();

});
</script>

     


