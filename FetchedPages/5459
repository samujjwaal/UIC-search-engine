var parsedData = JSON.parse(responseData.replace(/&quot;/g,'"'));
console.log(parsedData);

$("input:radio[name=CurrentlyNurse]").click(function() {
    if($(this).val() == 'Yes'){
        $('#degrees_rn').show();
        $('#degrees_no_rn').hide();
    }else{
        $('#degrees_rn').hide();
        $('#degrees_no_rn').show();
    }

});


$('#degrees_no_rn1').change(function(){
    var degrees_no_rn1 = $(this).val();
    
    $('#specialty_no_rn1').find('option').remove().end().append('<option value="">Select Specialty</option>');
    if(parsedData.degrees_no_rn[degrees_no_rn1].specialties != undefined){
        var specialties = parsedData.degrees_no_rn[degrees_no_rn1].specialties.values;
        for(var i=0;i<specialties.length;i++){
            $('#specialty_no_rn1').append($("<option/>", {
                value: specialties[i].value,
                text: specialties[i].display
            }));
        }    
    }

    $('#campus_no_rn1').find('option').remove().end().append('<option value="">Select Campus</option>');
    if(parsedData.degrees_no_rn[degrees_no_rn1].campuses.values != undefined){
        var campuses = parsedData.degrees_no_rn[degrees_no_rn1].campuses.values;
        for(var i=0;i<campuses.length;i++){
            $('#campus_no_rn1').append($("<option/>", {
                value: campuses[i].value,
                text: campuses[i].display
            }));
        }    
    }
});

