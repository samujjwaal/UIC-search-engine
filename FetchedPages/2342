function resizeMap() {
	var map = jQuery('#lmm_ea3a5c05');
		var map_parent_size = jQuery('#lmm_ea3a5c05').parent().width();
	if( (map_parent_size >0) && (map_parent_size < 605) ) {
		map.css({ 'width': '100%'});
		markermap_34.invalidateSize();
	}
}
resizeMap();
if (typeof jQuery().modal == 'function') {
		markermap_34.invalidateSize();
		jQuery('a[data-toggle="tab"],.tabbed-area a,.nav-tabs a').on('shown.bs.tab', function (e) {
			markermap_34.invalidateSize();
		});
	}
	if (typeof jQuery.ui != 'undefined') {
	markermap_34.invalidateSize();
	jQuery('.ui-tabs').on('tabsactivate', function(event, ui) {
		markermap_34.invalidateSize();
	});
}
if (typeof jQuery.mobile != 'undefined') {
	jQuery(document).bind('pageinit', function( event, data ){
		markermap_34.invalidateSize();
	});
}
});

/* ]]> */
</script>
<script type='text/javascript' src='https://dscc.uic.edu/wp-content/plugins/leaflet-maps-marker/inc/js/show_map.js?ver=3.12.2'></script>
</body>
</html>
