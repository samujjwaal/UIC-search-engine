function resizeMap() {
	var map = jQuery('#lmm_4efb3598');
		var map_parent_size = jQuery('#lmm_4efb3598').parent().width();
	if( (map_parent_size >0) && (map_parent_size < 830) ) {
		map.css({ 'width': '100%'});
		layermap_1.invalidateSize();
	}
}
