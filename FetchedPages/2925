// DOM ready
jQuery(document).ready(function(){

// Create the dropdown base
jQuery("<select />").appendTo("#topmenu");
jQuery("<select />").appendTo("#globalmenu");

// Create default option "Go to..."
jQuery("<option />", {
 "selected": "selected",
 "value"   : "",
 //"text"    : "Menu Selection..."
 "text"    : "Campuses and Colleges..."
}).appendTo("#topmenu select");
jQuery("<option />", {
 "selected": "selected",
 "value"   : "",
 "text"    : "Menu Selection..."
}).appendTo("#globalmenu select");

// Populate dropdown with menu items
/*jQuery("#topmenu a").each(function() {
var el = jQuery(this);
jQuery("<option />", {
   "value"   : el.attr("href"),
   "text"    : el.text()
}).appendTo("#topmenu select");
});*/

jQuery("#globalmenu a").each(function() {
var el = jQuery(this);
if ( el.parent().parent().attr("class") != "sub-menu" ){
jQuery("<option />", {
   "value"   : el.attr("href"),
   "text"    : el.text()
}).appendTo("#globalmenu select");
}
});

jQuery("#topmenu a").each(function() {
var el = jQuery(this);
if ( el.parent().parent().attr("id") == "menu-eyebrow" ){
jQuery("<option />", {
  "value"   : el.attr("href"),
  "text"    : el.text()
}).appendTo("#globalmenu select");
}
});

// To make dropdown actually work
// To make more unobtrusive: http://css-tricks.com/4064-unobtrusive-page-changer/
jQuery("#topmenu select").change(function() {
window.location = jQuery(this).find("option:selected").val();
});
jQuery("#globalmenu select").change(function() {
window.location = jQuery(this).find("option:selected").val();
});
jQuery(".tweet").tweet({
            username: "",
