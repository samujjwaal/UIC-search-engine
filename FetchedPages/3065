    <div class="component-description">
                    <h2 id="tips-for-writing-in-a-foreign-language" class="__title visible">Tips for Writing in a Foreign Language</h2>
                    </div>


  <div class="video-container">
    <iframe width="640" height="360" src="https://www.youtube.com/embed/videoseries?wmode=transparent&amp;rel=0&amp;list=PLxKSAHUDjL6TN1UXO4LppkvuFWWRNKDUM" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
