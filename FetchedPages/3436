<!-- /General helper -->
sr.reveal('.scroll-reveal', {viewport: window.document.documentElement, duration: 450, viewFactor: 0.05,reset: false, easing: 'cubic-bezier(0.420, 0.000, 0.200, 0.990)',distance: '20px',scale: 1 });
