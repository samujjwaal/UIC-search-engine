<!-- Calculation Trigger Fields -->
<script type="text/javascript">
function ctf(t,c){
  if(t==''){return true;}var o=new Object();
  if(typeof o[t]=='undefined') return true;
  var oa=o[t].split('|');for(var i=0;i<oa.length;i++){if(typeof o[oa[i]]!='undefined'){return true;}}
  return (o[t].indexOf('|'+c+'|')>-1);
}
</script>
<!-- Calculations -->
<script type="text/javascript">
function calculate(t,isOnPageLoad){
  var a,b,c;if(typeof t=='undefined') t='';if(typeof isOnPageLoad=='undefined') isOnPageLoad=false;
  try{updateCalcPipingReceivers()}catch(e){}
  return false;
}
calcErrExist = calculate('',true);
</script>
<script type="text/javascript">
if(calcErrExist){calcErr2()}
</script>

<!-- Branching Logic Trigger Fields -->
<script type="text/javascript">
function dbtf(t,c){
  if(t==''){return true;}var o=new Object();
  o.sub_action_type='|sub_request_note|sub_exp_type|sub_other_exp|source_pt_initials|source_pt_mrn|sub_ppe_worn|sub_oth_ppe|sub_exp_date|expose_dt_only|sub_symptoms|sub_other_symptoms|symptom_date_only|sub_comorb|sub_oth_comorb|';
  o.sub_exp_type='|sub_other_exp|source_pt_initials|source_pt_mrn|sub_ppe_worn|sub_oth_ppe|expose_dt_only|symptom_date_only|';
  o.sub_ppe_worn='|sub_oth_ppe|';
  o.show_combined_date='|sub_exp_date|';
  o.sub_symptoms='|sub_other_symptoms|symptom_date_only|';
  o.sub_comorb='|sub_oth_comorb|';
  if(typeof o[t]=='undefined') return true;
  var oa=o[t].split('|');for(var i=0;i<oa.length;i++){if(typeof o[oa[i]]!='undefined'){return true;}}
  return (o[t].indexOf('|'+c+'|')>-1);
}
</script>
<!-- Branching Logic -->
<script type="text/javascript">
function doBranching(t,b,isOnPageLoad){
  if(typeof isOnPageLoad=='undefined') isOnPageLoad=false;
  var c;if(typeof b=='undefined') b=false;if(typeof t=='undefined') t='';
  c='sub_request_note';try{if(dbtf(t,c))evalLogic(c,b,(document.form.sub_action_type.value=='2' || document.form.sub_action_type.value=='3'))}catch(e){brErr(c)}
  c='sub_exp_type';try{if(dbtf(t,c))evalLogic(c,b,(document.form.sub_action_type.value!='2' && document.form.sub_action_type.value!='3'))}catch(e){brErr(c)}
  c='sub_other_exp';try{if(dbtf(t,c))evalLogic(c,b,((( document.forms['form'].elements['__chk__sub_exp_type_RC_5'].value=='' ? 0 : 1))=='1'))}catch(e){brErr(c)}
  c='source_pt_initials';try{if(dbtf(t,c))evalLogic(c,b,((( document.forms['form'].elements['__chk__sub_exp_type_RC_2'].value=='' ? 0 : 1))=='1' || (( document.forms['form'].elements['__chk__sub_exp_type_RC_3'].value=='' ? 0 : 1))=='1'))}catch(e){brErr(c)}
  c='source_pt_mrn';try{if(dbtf(t,c))evalLogic(c,b,((( document.forms['form'].elements['__chk__sub_exp_type_RC_2'].value=='' ? 0 : 1))=='1' || (( document.forms['form'].elements['__chk__sub_exp_type_RC_3'].value=='' ? 0 : 1))=='1'))}catch(e){brErr(c)}
  c='sub_ppe_worn';try{if(dbtf(t,c))evalLogic(c,b,((( document.forms['form'].elements['__chk__sub_exp_type_RC_2'].value=='' ? 0 : 1))=='1' || (( document.forms['form'].elements['__chk__sub_exp_type_RC_3'].value=='' ? 0 : 1))=='1'))}catch(e){brErr(c)}
  c='sub_oth_ppe';try{if(dbtf(t,c))evalLogic(c,b,((( document.forms['form'].elements['__chk__sub_ppe_worn_RC_8'].value=='' ? 0 : 1))=='1'))}catch(e){brErr(c)}
